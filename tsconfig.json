{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2021",
    "module": "commonjs",
    "strict": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "incremental": true
  },
  "exclude": ["node_modules", "prod_node_modules"]
}
